<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders>
		<Console name="console" target="SYSTEM_OUT" follow="true">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{80} - %msg%n" />

		</Console>
	</appenders>
	<loggers>
		<root level="DEBUG">
			<appender-ref ref="console" />
		</root>
	</loggers>
</configuration>